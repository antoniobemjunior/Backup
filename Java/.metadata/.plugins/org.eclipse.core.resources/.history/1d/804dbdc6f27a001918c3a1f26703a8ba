package cargos;

public class Analista extends Cargo{

	//Construtor
	public Analista(double salario, int nivel) {
		cargo = "Analista";
		this.salario = salario;
		this.nivel = nivel;
	}
		
		
	//Imposto de Renda
	public void ir() {
		if(nivel == 1) System.out.println("Imposto de Renda: "+salario*0.15);
		if(nivel > 1) System.out.println("Imposto de Renda: "+salario*0.20);
	}
	
}
