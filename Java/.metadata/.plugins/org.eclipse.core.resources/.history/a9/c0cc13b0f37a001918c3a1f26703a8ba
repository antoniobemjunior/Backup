package principal;

import cargos.*;

public class Principal {

	public static void main(String[] args) {

		//Instanciar objetos
		Estagiario e = new Estagiario();
		Analista a = new Analista(5000,2);
		Desenvolvedor d = new Desenvolvedor(3000,3);
		
		//Chamando os métodos de indetificacao
		e.identificacao();
		a.identificacao();
		d.identificacao();
		
	}

}
