package formulario;

import javax.swing.*;

public class Formulario extends JFrame {

	JPanel contentPane = new JPanel();
	
	public void Formulario() {
		
		setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		setSize(590, 625);
		setResizable(false);
		setLocationRelativeTo(null);
		setContentPane(contentPane);
		
	}
	
}
